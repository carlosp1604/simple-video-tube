{
  "name": "video-tube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker-compose up -d",
    "db:make-migration": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex migrate:make -x ts",
    "db:migrate": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex migrate:latest",
    "db:rollback": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex migrate:rollback",
    "db:make-seed": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex seed:make -x ts",
    "db:seed": "TS_NODE_COMPILER_OPTIONS='{ \"isolatedModules\": false, \"module\": \"commonjs\" }' knex seed:run",
    "db:down": "docker-compose down",
    "db:destroy": "yarn db:down && sudo rm -rf ./docker/db",
    "db:reset": "yarn db:destroy && yarn db:up && sleep 10 && yarn db:migrate && yarn db:seed"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/knex": "^0.16.1",
    "@types/luxon": "^3.1.0",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-icons": "^3.0.0",
    "@types/react-transition-group": "^4.4.5",
    "@types/sanitizer": "^0.0.28",
    "@types/video.js": "^7.3.50",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "knex": "^2.3.0",
    "luxon": "^3.1.1",
    "mysql2": "^2.3.3",
    "next": "13.0.6",
    "next-auth": "^4.18.0",
    "objection": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.1.4",
    "react-icons": "^4.7.1",
    "react-transition-group": "^4.4.5",
    "sanitizer": "^0.1.3",
    "sass": "^1.57.1",
    "tailwind-scrollbar-hide": "^1.1.7",
    "ts-node": "^10.9.1",
    "typescript": "*",
    "video.js": "^7.21.1",
    "zod": "^3.20.2"
  },
  "main": "index.js",
  "repository": "git@gitlab.com:carlosp1022/video-tube.git",
  "author": "Carlos Ponton <carlitosponton@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.2.0",
    "eslint-config-next": "13.0.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
